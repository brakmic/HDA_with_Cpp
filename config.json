{
  "listeners": [
    {
      "address": "127.0.0.1",
      "port": 3000,
      "https": false
    }
  ],
  "app": {
    "document_root": "./builddir",
    "number_of_threads": 0,
    "static_files_cache_time": 30,
    "file_types": [
      "gif",
      "png",
      "jpg",
      "js",
      "css",
      "html",
      "ico",
      "swf",
      "xap",
      "apk",
      "cur",
      "xml",
      "mp4",
      "webp",
      "woff2",
      "ico"
    ],
    "use_brotli": true,
    "log": {
      "log_size_limit": 100000000,
      "log_level": "DEBUG"
  }
  }
  
}
